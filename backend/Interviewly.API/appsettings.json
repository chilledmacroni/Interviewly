{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GeminiSettings": {
    "ApiKey": "YOUR_GEMINI_API_KEY_HERE",
    "ModelName": "gemini-2.5-flash"
  },
  "MongoDbSettings": {
    "ConnectionString": "YOUR_MONGODB_CONNECTION_STRING_HERE",
    "DatabaseName": "InterviewlyDB"
  },

  "Crawl4AISettings": {
    "BaseUrl": "http://127.0.0.1:8000"
  },

  "JwtSettings": {
    "Secret": "YOUR_JWT_SECRET_AT_LEAST_32_CHARS_LONG",
    "Issuer": "InterviewlyAPI",
    "Audience": "InterviewlyClient",
    "ExpiryMinutes": 60
  }
}